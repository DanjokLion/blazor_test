{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Components.Web.ErrorBoundary": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Debug",
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ]
  },

  "ServiceType": 2203,
  "AllowedHosts": "*"
}
